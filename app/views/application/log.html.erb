<% 
id=request.original_url 

id=id.split('.')[-1]

@all_logs=Log.where(user_id: id)
@all_logs=@all_logs.order('id DESC')

@all_logs.each do |t| %>
            

 <div class="content-section-a">

        <div class="container">
            <div class="row">
                <div class="col-lg-5 col-sm-6">
                    <hr class="section-heading-spacer">
                    <div class="clearfix"></div>

                  
                    	<% user=User.find(t.user_id)

                    	  %>
                        <%="User_id: "+t.user_id + ". "+user.name+" "+user.surname %>
                        
                    
                    <p class="lead"><%=t.user_ip %><br/>
                    		<%=t.action %><br/>
                    		<%=t.time%>
                     </p>
                </div>
               

                
            </div>

        </div>
        <!-- /.container -->

    </div>
    <!-- /.content-section-a -->
    <%end%>